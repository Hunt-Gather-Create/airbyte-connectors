{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "project": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "revision": {
          "type": "number"
        },
        "visibility": {
          "type": "string"
        },
        "lastUpdateTime": {
          "type": "string"
        }
      }
    },
    "defaultBranch": {
      "type": "string"
    },
    "size": {
      "type": "number"
    },
    "remoteUrl": {
      "type": "string"
    },
    "sshUrl": {
      "type": "string"
    },
    "webUrl": {
      "type": "string"
    },
    "isDisabled": {
      "type": "boolean"
    },
    "branches": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "aheadCount": {
            "type": "number"
          },
          "behindCount": {
            "type": "number"
          },
          "isBaseVersion": {
            "type": "boolean"
          },
          "commits": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "commitId": {
                  "type": "string"
                },
                "comment": {
                  "type": "string"
                },
                "url": {
                  "type": "number"
                },
                "remoteUrl": {
                  "type": "number"
                },
                "author": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "date": {
                      "type": "number"
                    }
                  }
                },
                "committer": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "date": {
                      "type": "number"
                    }
                  }
                },
                "changeCounts": {
                  "type": "object",
                  "properties": {
                    "Add": {
                      "type": "number"
                    },
                    "Edit": {
                      "type": "number"
                    },
                    "date": {
                      "Delete": "number"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

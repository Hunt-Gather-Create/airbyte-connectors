{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Azure Pipeline Spec",
    "type": "object",
    "required": [
      "access_token",
      "organization",
      "project",
      "start_date"
    ],
    "additionalProperties": false,
    "properties": {
      "access_token": {
        "type": "string",
        "title": "access token of azure",
        "airbyte_secret": true
      },
      "organization": {
        "type": "string",
        "title": "organization"
      },
      "project": {
        "type": "string",
        "title": "project of microsoft azure"
      }, 
      "start_date": {
        "type": "string",
        "title": "Start Date",
        "description": "The date from which you'd like to replicate data for Backlog in the ISO 8601 format, i.e YYYY-MM-DDTmm:HH:ssZ. All data generated after this date will be replicated.",
        "examples": [
          "2010-03-27T14:03:51-0800"
        ],
        "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z$"
      },
      "api_version": {
        "type": "string",
        "title": "api version",
        "default": "6.0"
      }
    }
  }
}

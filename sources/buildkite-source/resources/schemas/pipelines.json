{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": true,
    "properties": {
        "id": {
            "type": "string"
        },
        "uuid": {
            "type": "string"
        },
        "slug": {
            "type": "string"
        },
        "name": {
            "type": "number"
        },
        "url": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "repository": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                },
                "provider": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "createdAt": {
            "type": "string"
        },
        "organization": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                }
            }
        }
    }
}
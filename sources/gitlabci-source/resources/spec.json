{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Gitlab CI Spec",
    "type": "object",
    "required": ["token", "groupName", "projects"],
    "additionalProperties": false,
    "properties": {
      "token": {
        "type": "string",
        "title": "Access Token",
        "airbyte_secret": true
      },
      "groupName": {
        "type": "string",
        "title": "Group Name",
        "airbyte_secret": true
      },
      "projects": {
        "type": "array",
        "title": "Project Names",
        "airbyte_secret": true
      }
    }
  }
}
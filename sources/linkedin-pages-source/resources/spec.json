{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "LinkedIn Pages Spec",
    "type": "object",
    "required": [
      "id",
      "secret",
      "token"
    ],
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "string",
        "title": "Client ID",
        "examples": [
          "g7rfr54fyguo8u"
        ]
      },
      "secret": {
        "type": "string",
        "title": "Client Secret",
        "examples": [
          "f9djn3jduee8t9s2"
        ],
        "airbyte_secret": true
      },
      "token": {
        "type": "string",
        "title": "Access Token",
        "airbyte_secret": true
      },
      "start_time": {
        "type": "string",
        "format": "datetime",
        "airbyte_type": "timestamp_without_timezone",
        "title": "Start Time",
        "description": "Test description"
      }
    }
  }
}